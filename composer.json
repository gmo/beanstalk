{
	"name": "gmo/beanstalk",
	"description": "Common beanstalk worker abstraction and queue utilities",
	"require": {
		"psr/log": "1.0.0",
		"pda/pheanstalk": "~3.0",
		"gmo/common": "~1.7",
		"nikic/php-parser": "~0.9"
	},
	"require-dev": {
		"phpunit/phpunit": "3.7.*",
		"gmo/dependency-injection": "~1.0"
	},
	"suggest": {
		"gmo/dependency-injection": "Provides a service provider for dependency injection"
	},
	"autoload": {
		"psr-4": {
			"GMO\\Beanstalk\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-0": {
			"": "tests/"
		}
	},
	"repositories": [
		{ "type": "composer", "url": "composer.gmodev.com" }
	],
	"archive": {
		"exclude": [
			"node_modules/",
			"package.json",
			"tests"
		]
	}
}
