{
    "name": "gmo/beanstalk",
    "license": "MIT",
    "description": "Common beanstalk worker abstraction and queue utilities",
    "require": {
        "php": "^7.1",
        "ext-pcntl": "*",
        "bolt/collection": "^1.1",
        "bolt/common": "^1.0@dev",
        "gmo/common": "^2.0@dev",
        "nesbot/carbon": "~1.19",
        "nikic/php-parser": "^3.0 || ^4.0",
        "pda/pheanstalk": "~3.0",
        "pimple/pimple": "^1.0 || ^3.0",
        "psr/container": "^1.0",
        "psr/log": "^1.0",
        "symfony/console": "^2.8 || ^3.0 || ^4.0",
        "symfony/finder": "^2.5 || ^3.0 || ^4.0",
        "symfony/var-dumper": "^2.6 || ^3.0 || ^4.0"
    },
    "require-dev": {
        "monolog/monolog": "~1.0",
        "phpunit/phpunit": "^6.2",
        "symfony/phpunit-bridge": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Gmo\\Beanstalk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gmo\\Beanstalk\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/queue"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
